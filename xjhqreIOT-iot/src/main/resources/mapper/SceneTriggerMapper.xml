<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xjhqre.iot.mapper.SceneTriggerMapper">


    <select id="listBySceneId" resultType="com.xjhqre.iot.domain.entity.SceneTrigger">
        select *
        from iot_scene_trigger
        where scene_id = #{sceneId}
    </select>
</mapper>